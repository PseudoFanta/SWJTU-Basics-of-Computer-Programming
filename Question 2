#include <iostream>
using namespace std;

void main(void)
{
	cout << "2018000000" << "    " << "PseudoFanta" << endl;

	//输出狮子图案
	cout << "             ,%%%%%%" << endl;
	cout << "           ,%%/\\%%%%/\\%" << endl;
	cout << "          ,%%%\\c "" J/%%%" << endl;
	cout << " %.       %%%%/ o  o \\%%%" << endl;
	cout << " `%%.     %%%%    _  |%%" << endl;
	cout << "  `%%     `%%%%(__Y__)%" << endl;
	cout << "  //       ;%%%%`\\-/%%%'" << endl;
	cout << " ((       /  `%%%%%%%'" << endl;
	cout << "  \\\\    .'          |" << endl;
	cout << "   \\\\  /       \\  | |" << endl;
	cout << "    \\\\/         ) | |" << endl;
	cout << "     \\         /_ | |__" << endl;
	cout << "     (___________)))))))" << endl;

	system("pause");

}
